specn = {"Black Demonology Necromancy Disease",
"Red Anarchy Blood Fire",
"Neutral Bashing Finesse",
"Green Balance Feral Growth",
"White Discipline Ninjutsu Strength",
"Purple Past Present Future",
"Blue Truth Peace Law"}
specs = {} -- spec has codex, hero, tile, color.
colors = {} -- color has workers, starting decks, specs (latter as strings for reference, not ids)
for _,i in ipairs(specn) do
  line = {}
  for word in i:gmatch("%w+") do table.insert(line, word) end
  linecolor = line[1]
  colors[linecolor] = {Worker = "", first = "", Specs = {}}
  for j = 2, #line do
    specs[line[j]] = {Codex = "", Hero = "", Spec = "", Color = linecolor}
    table.insert(colors[linecolor].Specs, line[j])
  end
end
