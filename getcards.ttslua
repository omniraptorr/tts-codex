allzone = getObjectFromGUID('283ed7').getObjects() -- big zone with all the cardsa
for _,i in ipairs(allzone) do
  isplit = {}
  for word in i.getName():gmatch("%w+") do table.insert(isplit, word) end
  nom = isplit[1]
  cat = isplit[2]
  if specs[nom] then
    specs[nom][cat] = i.guid
  elseif colors[nom] then
    colors[nom][cat] = i.guid
  end
end
